{
  "flex-layout.row#breadcrumb": {
    "children": ["flex-layout.col#breadcrumb"],
    "props": {
      "blockClass": ["breadcrumb", "product"]
    }
  },
  "flex-layout.col#breadcrumb": {
    "children": ["breadcrumb#default"]
  },
  "breadcrumb#default": {
    "props": {
      "blockClass": ["default"],
      "showOnMobile": true,
      "homeIconSize": 18
    }
  }
}
